      "source": [
        "## 1.1. BERT-CRF Model Definition"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "bert_crf_implementation"
      },
      "outputs": [],
      "source": [
        "class BertCrfForTokenClassification(nn.Module):\n",
        "    \"\"\"BERT model with CRF layer for token classification.\n",
        "\n",
        "    This model combines BERT embeddings with a CRF layer to enforce\n",
        "    valid BIO sequence constraints and improve performance on\n",
        "    imbalanced entity classes.\n",
        "    \"\"\"\n",
        "\n",
        "    def __init__(self, model_name: str, num_labels: int, dropout_rate: float = 0.1):\n",
        "        super().__init__()\n",
        "\n",
        "        # Load BERTic moel (without classification head)\n",
        "        self.bert = AutoModel.from_pretrained(model_name)\n",
        "        self.dropout = nn.Dropout(dropout_rate)\n",
        "\n",
        "        # Classification layer\n",
        "        self.classifier = nn.Linear(self.bert.config.hidden_size, num_labels)\n",
        "\n",
        "        # CRF layer\n",
        "        self.crf = CRF(num_labels, batch_first=True)\n",
        "\n",
        "        self.num_labels = num_labels\n",
        "\n",
        "        # Store config for compatibility with Trainer\n",
        "        self.config = self.bert.config\n",
        "        self.config.num_labels = num_labels\n",
        "\n",
        "    def forward(self, input_ids, attention_mask=None, labels=None, **kwargs):\n",
        "        # Get BERT embeddings (request hidden_states and attentions)\n",
        "        outputs = self.bert(\n",
        "            input_ids=input_ids,\n",
        "            attention_mask=attention_mask,\n",
        "            output_hidden_states=True,\n",
        "            output_attentions=True\n",
        "        )\n",
        "        sequence_output = outputs.last_hidden_state\n",
        "\n",
        "        # Apply dropout and classification layer\n",
        "        sequence_output = self.dropout(sequence_output)\n",
        "        logits = self.classifier(sequence_output)\n",
        "\n",
        "        outputs = {'logits': logits}\n",
        "\n",
        "        if labels is not None:\n",
        "            # Combine label mask with attention mask\n",
        "            label_mask = (labels != -100)\n",
        "            if attention_mask is not None:\n",
        "                # Both conditions must be true: valid label AND not padding\n",
        "                mask = label_mask & attention_mask.bool()\n",
        "            else:\n",
        "                mask = label_mask\n",
        "\n",
        "            # Ensure first token is always unmasked (CRF requirement)\n",
        "            mask[:, 0] = True\n",
        "\n",
        "            # Replace -100 with 0 for CRF (CRF expects labels in [0, num_labels-1])\n",
        "            crf_labels = labels.clone()\n",
        "            crf_labels[labels == -100] = 0  # Use 'O' tag (index 0) for padding\n",
        "\n",
        "            # Ensure tensors are on the same device\n",
        "            logits = logits.to(labels.device)\n",
        "            mask = mask.to(labels.device)\n",
        "            crf_labels = crf_labels.to(labels.device)\n",
        "\n",
        "            # Calculate CRF loss\n",
        "            # Note: CRF loss is negative log-likelihood, so we negate it\n",
        "            loss = -self.crf(logits, crf_labels, mask=mask, reduction='mean')\n",
        "        else:\n",
        "            loss = None\n",
        "\n",
        "        return TokenClassifierOutput(\n",
        "            loss=loss,\n",
        "            logits=logits,\n",
        "            hidden_states=getattr(outputs, 'hidden_states', None),\n",
        "            attentions=getattr(outputs, 'attentions', None),\n",
        "        )\n",
        "\n",
        "    def predict(self, input_ids, attention_mask=None):\n",
        "        \"\"\"Predict using CRF Viterbi decoding\"\"\"\n",
        "        with torch.no_grad():\n",
        "            outputs = self.bert(input_ids=input_ids, attention_mask=attention_mask)\n",
        "            sequence_output = outputs.last_hidden_state\n",
        "            sequence_output = self.dropout(sequence_output)\n",
        "            logits = self.classifier(sequence_output)\n",
        "\n",
        "            # Prepare mask for CRF decoding\n",
        "            if attention_mask is not None:\n",
        "                mask = attention_mask.bool()\n",
        "                # Ensure first token is always unmasked (CRF requirement)\n",
        "                mask[:, 0] = True\n",
        "            else:\n",
        "                # If no attention mask provided, assume all tokens are valid\n",
        "                mask = torch.ones(input_ids.shape, dtype=torch.bool, device=input_ids.device)\n",
        "\n",
        "            # Ensure tensors are on the same device\n",
        "            logits = logits.to(input_ids.device)\n",
        "            mask = mask.to(input_ids.device)\n",
        "\n",
        "            # Use CRF to decode the best sequence\n",
        "            predictions = self.crf.decode(logits, mask=mask)\n",
        "            return predictions"
      ]
    },

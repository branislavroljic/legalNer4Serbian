        "def create_model_and_trainer(fold_num, train_dataset, val_dataset, data_collator, tokenizer, ner_dataset, device):\n",
        "    \"\"\"\n",
        "    Create a fresh BERT-CRF model and trainer for each fold.\n",
        "    \n",
        "    Args:\n",
        "        fold_num: Current fold number\n",
        "        train_dataset: Training dataset for this fold\n",
        "        val_dataset: Validation dataset for this fold\n",
        "        data_collator: Data collator\n",
        "        tokenizer: Tokenizer\n",
        "        ner_dataset: NER dataset with label mappings\n",
        "        device: Device to use (cuda/cpu)\n",
        "    \n",
        "    Returns:\n",
        "        model, trainer, fold_output_dir\n",
        "    \"\"\"\n",
        "    print(f\"Creating model and trainer for fold {fold_num}...\")\n",
        "    \n",
        "    # Create fresh model for this fold\n",
        "    model = BertCrfModel(\n",
        "        model_name=MODEL_NAME,\n",
        "        num_labels=ner_dataset.get_num_labels(),\n",
        "        dropout_rate=0.1\n",
        "    )\n",
        "    \n",
        "    # Move model to device\n",
        "    model.to(device)\n",
        "    \n",
        "    # Set up model configuration\n",
        "    model.config = model.bert.config\n",
        "    model.config.id2label = ner_dataset.id_to_label\n",
        "    model.config.label2id = ner_dataset.label_to_id\n",
        "    model.config.num_labels = ner_dataset.get_num_labels()\n",
        "    \n",
        "    print(f\"Model created with {ner_dataset.get_num_labels()} labels\")\n",
        "    print(f\"Model moved to device: {device}\")\n",
        "\n",
        "    # Define evaluation metrics function for BERT-CRF\n",
        "    def compute_metrics(eval_pred):\n",
        "        predictions, labels = eval_pred\n",
        "\n",
        "        # For CRF model, predictions are already decoded (no need for argmax)\n",
        "        # Check if predictions need argmax (standard model) or are already decoded (CRF)\n",
        "        if len(predictions.shape) == 3:  # Standard model output (batch, seq, num_labels)\n",
        "            predictions = np.argmax(predictions, axis=2)\n",
        "        # else: CRF predictions are already decoded as (batch, seq)\n",
        "\n",
        "        # Remove ignored index (special tokens)\n",
        "        true_predictions = [\n",
        "            [ner_dataset.id_to_label[p] for (p, l) in zip(prediction, label) if l != -100]\n",
        "            for prediction, label in zip(predictions, labels)\n",
        "        ]\n",
        "        true_labels = [\n",
        "            [ner_dataset.id_to_label[l] for (p, l) in zip(prediction, label) if l != -100]\n",
        "            for prediction, label in zip(predictions, labels)\n",
        "        ]\n",
        "\n",
        "        # Compute metrics using seqeval\n",
        "        results = {\n",
        "            \"precision\": precision_score(true_labels, true_predictions),\n",
        "            \"recall\": recall_score(true_labels, true_predictions),\n",
        "            \"f1\": f1_score(true_labels, true_predictions),\n",
        "            \"accuracy\": accuracy_score(true_labels, true_predictions),\n",
        "        }\n",
        "        return results\n",
        "\n",
        "    # Training configuration\n",
        "    fold_output_dir = f\"{OUTPUT_DIR}_fold_{fold_num}\"\n",
        "\n",
        "    training_args = TrainingArguments(\n",
        "        output_dir=fold_output_dir,\n",
        "        num_train_epochs=8,\n",
        "        per_device_train_batch_size=4,\n",
        "        per_device_eval_batch_size=4,\n",
        "        warmup_ratio=0.1,\n",
        "        weight_decay=0.01,\n",
        "        learning_rate=3e-5,\n",
        "        logging_dir=f'{fold_output_dir}/logs',\n",
        "        logging_steps=50,\n",
        "        eval_strategy=\"epoch\",\n",
        "        save_strategy=\"epoch\",\n",
        "        save_total_limit=2,\n",
        "        load_best_model_at_end=True,\n",
        "        metric_for_best_model=\"f1\",\n",
        "        greater_is_better=True,\n",
        "        push_to_hub=False,\n",
        "        dataloader_pin_memory=False,\n",
        "        gradient_accumulation_steps=2,\n",
        "        fp16=torch.cuda.is_available(),\n",
        "        report_to=None\n",
        "    )\n",
        "\n",
        "    # Initialize custom BERT-CRF trainer\n",
        "    trainer = BertCrfTrainer(\n",
        "        model=model,\n",
        "        args=training_args,\n",
        "        train_dataset=train_dataset,\n",
        "        eval_dataset=val_dataset,\n",
        "        tokenizer=tokenizer,\n",
        "        data_collator=data_collator,\n",
        "        compute_metrics=compute_metrics,\n",
        "        callbacks=[EarlyStoppingCallback(early_stopping_patience=3)],\n",
        "    )\n",
        "\n",
        "    print(f\"Trainer initialized for fold {fold_num}\")\n",
        "    return model, trainer, fold_output_dir\n",
        "\n",
        "print(\"✅ All K-fold helper functions defined successfully!\")"
      ]
